#editSubscriptionModal.modal.fade(tabindex='-1' role='dialog' aria-labelledby='exampleModalLabel' aria-hidden='true')
    .modal-dialog.modal-xl(role='document')
        .pt-5#spinner-div
            .spinner-border.text-primary(role='status' style="width:10rem; height: 10rem;")
        .modal-content
            .modal-header
                h5#editSubscriptionModalLabel.modal-title Subscription
                button.close(type='button' data-dismiss='modal' aria-label='Close')
                    span(aria-hidden='true') &times;
            .modal-body
                .alert#editSubscriptionModalAlert.alert-success.alert-dismissible(role='alert' hidden=true)
                        .li#message.text-center
                            | test
                include subscription-form-edit
            .modal-footer
                button.btn.btn-danger(type='button' data-dismiss='modal') Close
